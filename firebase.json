{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/deckRegistration/**",
        "destination": "/deckRegistration/[decDocId].html"
      },
      {
        "source": "/playRoom/**",
        "destination": "/playRoom/[roomId].html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}